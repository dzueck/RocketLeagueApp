{
    "manifest_version": 1,
    "type": "WebApp",
    "meta": {
        "name": "Rocket League Clock Catcher",
        "version": "1.0.0",
		"minimum-overwolf-version": "0.0.0",//This needs to be changed to aactual min
        "author": "Dylan Zueck & Carter Askins",
        "icon": "TempImage.png",//temp image
        "icon_gray": "TempImage.png",//temp image
        "description": "Collects cool time and score combinations after scoring in Rocket League."
    },
    "data": {
        "start_window": "Time Catcher",
		"game_events": [10798],
        "windows": {
            "Time Catcher": {
                "file": "Important_Files/Temp.html",
				"show_in_taskbar": true,
                "transparent": true,
                "resizable": true,
                "size": {"width": 1, "height": 1},
                "min_size": {"width": 1, "height": 1},
                "max_size": {"width": 2, "height": 2}
            }
			
        }
    }
}