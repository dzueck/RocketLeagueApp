{
    "manifest_version": 1,
    "type": "WebAp",
    "meta": {
        "name": "Rocket League Clock Catcher",
        "version": "1.0.0",
        "author": "Dylan Zueck & Carter Askins",
        "icon": "TempImage.png",
        "icon_gray": "TempImage.png",
        "description": "Collects cool time and score combinations after scoring in Rocket League."
    },
    "data": {
        "start_window": "MainWindow",
        "windows": {
            "MainWindow": {
                "file": "Important_Files/Temp.html",
                "transparent": true,
                "resizable": true,
                "size": {"width": 400, "height": 300},
                "min_size": {"width": 200, "height": 200},
                "max_size": {"width": 600, "height": 500}
            }
        }
    }
}